<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="admin"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyProperties,jcr:lockManagement]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="admin"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow1>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="administrators"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyProperties,jcr:lockManagement]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="administrators"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow3>
</jcr:root>
